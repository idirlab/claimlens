genius-referent[data-genius-featured-referent]{
    background-color: transparent;
	cursor: pointer;
	/*text-decoration: underline;*/
	border-bottom: thick solid red;
}
.com-genius-Genius-showing genius-referent[data-genius-featured-referent][data-genius-is-highlighted] {
    background-color:transparent;
}
genius-referent[data-genius-featured-referent][data-genius-hover=true]{
	background-color:transparent;
}
.com-genius-Genius-showing genius-referent[data-genius-featured-referent][data-genius-hover=true], genius-referent[data-genius-featured-referent][data-genius-hover=true]{
	background-color:transparent;
}
.com-genius-Genius-showing genius-referent[data-genius-featured-referent] {
    background-color: transparent;
}

.com-genius-Genius-showing genius-referent[data-genius-featured-referent][data-genius-referent-in-progress=true] {
    background-color: transparent;
}
@keyframes background_fade{0%{background-color:transparent}100%{background-color:transparent}}